    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Agentii</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/agencies/add" class="btn btn-white"><i class="fa fa-plus-circle"></i>&nbsp;Adauga agentie</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th style="width:10%">Cod</th>
							<th style="width:22%">Nume</th>
							<th style="width:15%">Cod fiscal</th>
							<th style="width:15%">Numar inregistrare</th>
							<th style="width:21%">Nume companie</th>
							<th style="width:5%">&nbsp;</th>
							<th style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle"><%= item.Code %></td>
							<td class="v-align-middle bold"><%= item.AgencyName %></td>
							<td class="v-align-middle"><span class="muted"><%= item.FiscalCode %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.RegisterCode %></span></td>
							<td class="v-align-middle"><span class="muted"><%= item.CompanyName %></span></td>
							<td><a href="/admin/agencies/edit/<%= item.ID %>" class="action-icon btn btn-small btn-white" title="Modifica"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/agencies/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-white"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>
