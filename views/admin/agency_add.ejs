    <div class="content">
      <div class="page-title"> <i class="icon-custom-left"></i>
        <h3><%- (req.param('id')?'Modificare':'Adaugare') %> agentie</h3>
      </div>
      <div class="row">
        <div class="col-md-12">
          <div class="grid simple">
            <div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
			</div>
            <div class="grid-body no-border">
			<form class="form-no-horizontal-spacing" id="user-form" method="post" autocomplete="off">	
              <div class="row column-seperation">
                <div class="col-md-6">
                  <h4>Informatii generale</h4>            
                    <div class="row form-row">
                      <div class="col-md-10">
						<label for="Name">Nume</label>
						<div class="widget-col">
							<input name="Name" id="Name" type="text" class="form-control required" value="<%- (typeof item!='undefined'?item.AgencyName:'') %>" />
						</div>
                      </div>
                      <div class="col-md-2">
						<label for="Code">Cod</label>
						<div class="widget-col">
							<input name="Code" id="Code" type="text" class="form-control required" value="<%- (typeof item!='undefined'?item.Code:'') %>" />
						</div>
                      </div>
                    </div>
                    <div class="row form-row">
                      <div class="col-md-12">
						<label for="CompanyName">Nume companie</label>
						<div class="widget-col">
							<input name="CompanyName" id="CompanyName" type="text" class="form-control required" value="<%- (typeof item!='undefined'?item.CompanyName:'') %>" />
						</div>
                      </div>
                    </div>
					<br />
                </div>
                <div class="col-md-6">
                  <h4>Detalii facturare</h4>            
                    <div class="row form-row">
                      <div class="col-md-6">
						<label for="FiscalCode">Cod fiscal</label>
						<div class="widget-col">
							<input name="FiscalCode" id="FiscalCode" type="text" class="form-control required" value="<%- (typeof item!='undefined'?item.FiscalCode:'') %>" />
						</div>
                      </div>
                      <div class="col-md-6">
						<label for="RegisterCode">Numar inregistrare</label>
						<div class="widget-col">
							<input name="RegisterCode" id="RegisterCode" type="text" class="form-control required" value="<%- (typeof item!='undefined'?item.RegisterCode:'') %>" />
						</div>
                      </div>
                    </div>
					<br />
                </div>
              </div>
				<div class="form-actions">
					<div class="pull-left">
					  <button class="btn btn-danger btn-cons" type="submit"><i class="icon-ok"></i> Salveaza</button>
					  <a class="btn btn-white btn-cons" href="/admin/agencies">Renunta</a>
					</div>
				  </div>
			</form>
            </div>
          </div>
        </div>
      </div>
    </div>
