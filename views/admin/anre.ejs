    <div class="content">
      <div class="page-title"> <i class="icon-custom-left"></i>
        <h3>Raport ANRE</h3>
      </div>
      <div class="row">
        <div class="col-md-12">
          <div class="grid simple">
            <div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
			</div>
            <div class="grid-body no-border">
				<form class="form-no-horizontal-spacing" id="anre-form" method="post" autocomplete="off">	
					<h4>Filtre</h4>            
                    <div class="row form-row">
						<div class="form-group col-md-3">
							<label class="form-label">Inceput</label>
							<div class="input-group">
								<input type="text" class="form-control required" id="prm_StartDate" name="prm_StartDate" data-date-format="yyyy-mm-dd" />
								<span class="input-group-addon primary">		
									<span class="arrow"></span>
									<i class="fa fa-align-justify"></i>		
								</span>
							</div>
						</div>
						<div class="form-group col-md-3">
							<label class="form-label">Sfarsit</label>
							<div class="input-group">
								<input type="text" class="form-control required" id="prm_EndDate" name="prm_EndDate" data-date-format="yyyy-mm-dd" />
								<span class="input-group-addon primary">		
									<span class="arrow"></span>
									<i class="fa fa-align-justify"></i>		
								</span>
							</div>
						</div>
                    </div>
					<div class="form-actions">
						<div class="pull-left">
							<button class="btn btn-danger btn-cons" type="submit">Download</button>
						</div>
					</div>
				</form>
            </div>
          </div>
        </div>
      </div>
    </div>
