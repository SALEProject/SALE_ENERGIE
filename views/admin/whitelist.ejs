    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Motive refuzare tranzactionare</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
				<a href="/admin/whitelist/add" class="btn btn-white"><i class="fa fa-plus-circle"></i>&nbsp;Adauga</a>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed" id="example-table">
					<thead>
						<tr>
							<th class="v-align-middle" style="width:10%">ID</th>
							<th class="v-align-middle" style="width:79%">Motiv</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
							<th class="v-align-middle" style="width:5%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle" style="width:10%"><%= item.ID %></td>
							<td class="v-align-middle" style="width:79%"><%= item.Reason %></td>
							<td><a href="/admin/whitelist/edit/<%= item.ID %>" class="action-icon btn btn-small btn-white" title="Modifica"><i class="fa fa-edit"></i></a></td>
							<td><a href="/admin/whitelist/delete/<%- item.ID %>" title="Sterge" class="action-icon delete-confirm btn btn-small btn-white"><i class="fa fa-trash-o"></i></a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>
