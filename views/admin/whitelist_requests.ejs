    <div class="content">
      <div class="page-title">
		<i class="icon-custom-left"></i>
        <h3>Cereri refuzare tranzactionare</h3>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <div class="grid simple">
			<div class="grid-title no-border">
				<%- toolsService.getFlashMessage(req.flash()) %>
			</div>
			<div class="grid-body no-border">
				<table class="table table-hover table-condensed admin-table" id="example-table">
					<thead>
						<tr>
							<th class="v-align-middle" style="width:8%">ID</th>
							<th class="v-align-middle" style="width:20%">Ring</th>
							<th class="v-align-middle" style="width:20%">De la</th>
							<th class="v-align-middle" style="width:20%">In privinta</th>
							<th class="v-align-middle" style="width:20%">Motiv</th>
							<th class="v-align-middle" style="width:10%">&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% if(items.length>0) { _.each(items,function(item) { %>
						<tr>
							<td class="v-align-middle" style="width:10%"><%= item.ID %></td>
							<td class="v-align-middle" style="width:20%"><%= item.Name %></td>
							<td class="v-align-middle" style="width:20%"><%= item.FromRequest %></td>
							<td class="v-align-middle" style="width:20%"><%= item.RegardingClient %></td>
							<td class="v-align-middle" style="width:20%"><%= item.Column1 %></td>
							<td style="width:10%"><a href="/admin/whitelist/approve/<%= item.ID %>" class="btn btn-white btn-small" title="Aproba">Aproba</a></td>
						</tr>
						<% }); } %>
					</tbody>
				</table>
			</div>
          </div>
        </div>
      </div>
    </div>
